paths=(
    /Applications/Visual\ Studio\ Code.app/Contents/Resources/app/bin
    "$HOME/.nvm/versions/node/$(nvm version)/bin/yarn"
    /opt/homebrew/opt/postgresql@16/bin
    ~/.bun/bin
    "${GOBIN}"
    ~/.dotfiles/bin
)

for dir in "${paths[@]}"; do
    if [[ -d "$dir" ]]; then
        PATH="$dir:$PATH"
    fi
done;
unset dir;
unset paths;

export PATH;
