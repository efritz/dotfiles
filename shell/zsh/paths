paths=(
    /Applications/Visual\ Studio\ Code.app/Contents/Resources/app/bin
    /opt/homebrew/opt/postgresql@16/bin
    "${GOBIN}"
    "{BUN_INSTALL}/bin"
    ~/.local/bin # pipx
    ~/.dotfiles/bin
)

for dir in "${paths[@]}"; do
    if [[ -d "$dir" ]]; then
        PATH="$dir:$PATH"
    fi
done;
unset dir;
unset paths;

export PATH;
