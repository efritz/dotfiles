{
    "name": "ai",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "yarn clean && tsc && yarn rename && yarn update-imports",
        "rename": "renamer --find .js --replace .mjs \"dist/**\"",
        "update-imports": "find dist -name '*.mjs' -exec sed -i '' -e \"s/from '\\.\\([^']*\\)'/from '\\.\\1.mjs'/g\" {} \\;",
        "start": "yarn build && node dist/main.mjs",
        "format": "prettier '**/{*.{ts,json},.*.cjs}' --list-different --write",
        "lint": "eslint"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.24.3",
        "@google/generative-ai": "^0.16.0",
        "@humanwhocodes/gitignore-to-minimatch": "^1.0.2",
        "@types/uuid": "^10.0.0",
        "chalk": "^5.3.0",
        "chokidar": "^3.6.0",
        "commander": "^12.1.0",
        "diff": "^5.2.0",
        "event-iterator": "^2.0.0",
        "glob": "^11.0.0",
        "groq-sdk": "^0.5.0",
        "minimatch": "^10.0.1",
        "ollama": "^0.5.6",
        "openai": "^4.52.7",
        "ora": "^8.0.1",
        "tree-kill": "^1.2.2",
        "uuid": "^10.0.0",
        "yaml": "^2.5.0"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
        "@types/diff": "^5.2.1",
        "@types/minimatch": "^5.1.2",
        "@types/node": "^20.14.12",
        "eslint": "^9.8.0",
        "eslint-plugin-unused-imports": "^4.0.1",
        "prettier": "^3.3.3",
        "renamer": "^5.0.2",
        "typescript": "^5.5.4",
        "zx": "^8.1.4"
    }
}
